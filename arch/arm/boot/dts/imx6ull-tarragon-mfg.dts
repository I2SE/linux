#include "imx6ull-tarragon-common.dtsi"
#include "imx6ull-tarragon-pmic.dtsi"

/ {
	model = "I2SE Tarragon Mfgtool";
	compatible = "fsl,imx6ull-tarragon", "fsl,imx6ull";
};

&iomuxc {
	pinctrl-0 = <&pinctrl_push_pull_cfg>;
};

&iomuxc_snvs {
	pinctrl-0 = <&init_GPIO_pinsSnvs>;
};

&usbotg1 {
	dr_mode = "peripheral";
	status = "okay";
};

&usdhc2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc2_4bit>;
	bus-width = <4>;
	broken-cd;
	non-removable;
	status = "okay";
};
